<%= form_with model: @listing do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title%>
	<br>
	<%= f.label :property_type%>
	<%= f.text_field :property_type%>
	<br>
	<%= f.label :location%>
	<%= f.text_field :location%>
	<br>
	<%= f.label :description%>
	<%= f.text_field :description%>
	<br>
	<%= f.label :capacity%>
	<%= f.text_field :capacity%>
	<br>
	
	<% if @listing.new_record? %>
			<%= f.submit'Create' %>
	<% else %>
			<%= f.submit 'Update'%>
	<% end %>
<%end%>
<br>
<a href="/">Home</a>